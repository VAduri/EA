            </apex:pageblockbuttons>

            <!-- **********   [Record Type : Master ]   **********  -->
            <apex:outputpanel >
                <apex:pageblocksection title="Case Information" showheader="false" collapsible="false" columns="2">
                    <apex:pageblocksectionitem >
                        <apex:outputlabel value="Case Owner"/>
                            <apex:outputpanel >
                            <apex:outputfield value="{!Case.OwnerId}"/>
                        </apex:outputpanel>
                    </apex:pageblocksectionitem>
                    <apex:outputfield value="{!Case.Status}"/>
                    <apex:outputfield value="{!Case.CaseNumber}"/>
                    <apex:outputfield value="{!Case.Priority}"/>
                    <apex:outputfield value="{!Case.ContactId}"/>
                    <apex:outputfield value="{!Case.Origin}"/>
                    <apex:outputfield value="{!Case.AccountId}"/>
                    <apex:outputfield value="{!Case.Reason}"/>
                    <apex:outputfield value="{!Case.Product__c}"/>
                    <apex:pageblocksectionitem />
                    <apex:outputfield value="{!Case.Type}"/>
                    <apex:pageblocksectionitem />
                    <apex:outputfield value="{!Case.XCaseNumber__c}"/>
                    <apex:pageblocksectionitem />
                </apex:pageblocksection>
                <apex:pageblocksection title="Web Information" showheader="false" collapsible="false" columns="2">
                    <apex:outputfield value="{!Case.SuppliedEmail}"/>
                    <apex:outputfield value="{!Case.SuppliedCompany}"/>
                    <apex:outputfield value="{!Case.SuppliedName}"/>
                    <apex:outputfield value="{!Case.SuppliedPhone}"/>
                    <apex:outputfield value="{!Case.CreatedDate}"/>
                    <apex:outputfield value="{!Case.ClosedDate}"/>
                </apex:pageblocksection>
                <apex:pageblocksection title="Additional Information" showheader="false" collapsible="false" columns="2">
                    <apex:outputfield value="{!Case.EngineeringReqNumber__c}"/>
                    <apex:outputfield value="{!Case.PotentialLiability__c}"/>
                    <apex:outputfield value="{!Case.SLAViolation__c}"/>
                    <apex:pageblocksectionitem />
                </apex:pageblocksection>
                <apex:pageblocksection title="System Information" showheader="false" collapsible="false" columns="2">
                    <apex:pageblocksectionitem >
                        <apex:outputlabel value="Created By"/>
                        <apex:outputpanel >
                            <apex:outputfield value="{!Case.CreatedById}"/>&nbsp;, 
                            <apex:outputfield value="{!Case.CreatedDate}"/>
                        </apex:outputpanel>
                    </apex:pageblocksectionitem>
                    <apex:pageblocksectionitem >
                        <apex:outputlabel value="Last Modified By"/>
                        <apex:outputpanel >
                            <apex:outputfield value="{!Case.LastModifiedById}"/>&nbsp;, 
                            <apex:outputfield value="{!Case.LastModifiedDate}"/>
                        </apex:outputpanel>
                    </apex:pageblocksectionitem>
                </apex:pageblocksection>
                <apex:pageblocksection title="Description Information" showheader="false" collapsible="false" columns="1">
                    <apex:outputfield value="{!Case.Subject}"/>
                    <apex:outputfield value="{!Case.Description}"/>
                </apex:pageblocksection>
            </apex:outputpanel>
        </apex:pageblock>
    </apex:form>

    <!-- **********   [Related Lists for Record Type : Master ]   **********  -->
    <apex:outputpanel >
        <apex:relatedlist list="CaseSolutions" title="Solutions"/>
        <apex:relatedlist list="OpenActivities" title="Open Activities"/>
        <apex:relatedlist list="ActivityHistories" title="Activity History"/>
        <apex:relatedlist list="CombinedAttachments" title="Attachments"/>
