<apex:page standardController="Account">
    <apex:form >
    <apex:pageBlock title="My Page Block">
        <apex:pageBlockButtons location="bottom">
            <apex:commandButton action="{!Save}" value="Save"/>
             <apex:commandButton action="{!QuickSave}" value="QuickSave"/>
        </apex:pageBlockButtons>
        <apex:pageBlockSection title="Aduri" collapsible="true" columns="1">
            <apex:pageBlockSectionItem >
                <apex:outputLabel value="Account Name"></apex:outputLabel>
                <apex:outputfield value="{!Account.Name}"/>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem >
                <apex:outputLabel value="Created Date"></apex:outputLabel>
                <apex:outputfield value="{!Account.createdDate}"/>
            </apex:pageBlockSectionItem>
      
                <apex:repeat value="{!$ObjectType.Account.FieldSets.New_Set}" var="Venkata"> 
            <apex:inputField value="{!Account[Venkata]}"/>
            </apex:repeat>
        </apex:pageBlockSection>
    </apex:pageBlock>
   </apex:form> 
</apex:page>